super + Tab
	bspc node -f older.window.local  || bspc node -f older.window
super + hyper + Tab
	bspc node -f next.window.local || bspc node -f older.window.local  || bspc node -f next.window

super + shift + Tab
	bspc node -f prev.window.local || bspc node -f older.window.local || bspc node -f prev.window
        
super + shift + {Down, Up}
	bspc node -f {next,prev}.window.local

super + shift + {Left, Right}
	bspc desktop -f {prev,next}


