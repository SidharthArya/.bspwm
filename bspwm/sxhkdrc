super + shift + Return
	~/.config/sxhkd/scripts/scratchpad.sh class Terminal st -c Terminal -T Terminal -e tmux

super + p
	rofi -show run -theme Pop-Dark -font 'Noto Sans 14'

super + shift + p
	rofi -show drun -theme Pop-Dark -font 'Noto Sans 14'

super + a
      sh ~/.config/sxhkd/scripts/run.sh

super + shift + r; r
	pkill -x panel; bspc wm -r

super + shift + r; w
	pkill -x panel; bspc wm -r

super + shift + r; k
	pkill -USR1 -x sxhkd

super + shift + q; q 
	sh ~/.config/sxhkd/scripts/session_exit.sh && bspc quit

super + shift + q; shift + q 
	killall emacs && fusermount -u ~/Documents && bspc quit

super + shift + q; p
	sh ~/.config/sxhkd/scripts/session_exit.sh && poweroff

super + shift + q; h
	sh ~/.config/sxhkd/scripts/session_exit.sh && systemctl hibernate -i

super + shift + q; s
	sh ~/.config/sxhkd/scripts/session_exit.sh && poweroff

super + shift + q; r
	sh ~/.config/sxhkd/scripts/session_exit.sh && reboot

super + shift + q; l
      dm-tool lock 
        
